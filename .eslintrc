{
  "parser": "babel-eslint",
  "plugins":
    ["babel"],
  "rules": {
    "strict": 0,
    "babel/semi": 1,
    "react/sort-comp": "off",
    "react/prop-types": "off",
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }]
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": "airbnb"
}
